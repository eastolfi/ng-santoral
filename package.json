{
  "name": "ng-santoral",
  "version": "0.0.0",
  "scripts": {
    "prisma": "prisma",
    "nx": "nx",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "prisma:link": "node scripts/link-prisma.js",
    "prisma:client": "npx prisma generate",
    "docker:backend": "npx nx docker-build backend",
    "docker:frontend": "npx nx docker-build frontend",
    "nest": "cd .\\apps\\backend && npx nest",
    "studio": "npx prisma studio",
    "backend": "--------------------------------------------",
    "backend:static": "npx copyfiles --flat .\\api\\src\\certs\\*.pem .\\api\\lib\\certs",
    "backend:compile": "shx rm -rf lib/ && tsc -p tsconfig.json",
    "backend:compile-with-static": "npm run api:compile && npm run api:static",
    "backend:start": "node lib/",
    "backend:dev": "nodemon -x ts-node src/index.ts",
    "backend:prettier": "npx prettier \"**/*.ts\" --write",
    "backend:mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "backend:test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "backend:bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "frontend": "--------------------------------------------",
    "frontend:ng": "ng",
    "frontend:start": "ng serve",
    "frontend:build": "ng build",
    "frontend:watch": "ng build --watch --configuration development",
    "frontend:test": "ng test"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
